{
   "version": "2.1.1",
   "compilerOptions": {
       "lib": ["es5", "es6"],
       "outDir": "dist",
       "target": "es6",
       "module": "commonjs",
       "moduleResolution": "node",
       "importHelpers": true,
       "emitDecoratorMetadata": true,
       "experimentalDecorators": true,
       "sourceMap": true,
       "noImplicitAny": true,
       "declaration": true,
       "noFallthroughCasesInSwitch": true,
       "noImplicitReturns": true,
       "stripInternal": true,
       "pretty": true,
       "strictNullChecks": true,
       "noUnusedLocals": true,
       "downlevelIteration": true
   },
   "exclude": [
       "tmp",
       "temp",
       "dist",
       "node_modules"
   ]
}